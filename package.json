    "react-infinite-scroll-component": "^6.1.0",
    "react-paginate": "^7.1.3",
    "react-router-dom": "^5.2.0",
    "react-scroll": "^1.8.4",
    "react-toastify": "^4.5.2",
    "sass-loader": "^9.0.3",
    "style-loader": "^0.23.1"
  },
